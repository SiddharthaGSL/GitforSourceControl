table "main"."staging_vatrecalreqdata" {
  columns = (
    column "selected" {
      type = bigint
    }
    column "sitecode" {
      type = bigint
    }
    column "sitename" {
      type = character varying(100)
    }
    column "requesteddate" {
      type = timestamp(0) without time zone
    }
    column "status" {
      type = bigint
    }
    column "statusdisplay" {
      type = character varying(100)
    }
    column "remarks" {
      type = character varying(500)
    }
  )
}

