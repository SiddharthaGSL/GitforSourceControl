view "main"."v_psite_alloc_article" {
  text = """-
SELECT psite_alloc_article.admsite_code AS site_code,
    psite_alloc_article.invarticle_code AS articleid
   FROM main.psite_alloc_article;
"""
  columns = (
    column "site_code" {
      type = "bigint"
    }
    column "articleid" {
      type = "bigint"
    }
  )
}

