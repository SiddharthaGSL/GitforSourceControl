view "main"."v_lpcard_distribution" {
  text = """-
SELECT lpcard_distribution.cardno,
    lpcard_distribution.admsite_code
   FROM main.lpcard_distribution;
"""
  columns = (
    column "cardno" {
      type = "character varying(30)"
    }
    column "admsite_code" {
      type = "bigint"
    }
  )
}

